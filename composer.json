{
    "name": "vemcogroup/laravel-weather",
    "description": "Weather package for Laravel to use different providers to get weather info",
    "keywords": [
        "darksy",
        "laravel",
        "weather",
        "weatherstack"
    ],
    "homepage": "https://github.com/vemcogroup/laravel-weather",
    "license": "MIT",
    "authors": [
        {
            "homepage": "https://vemcogroup.com",
            "name": "vemDevs"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "firebase/php-jwt": "^6.4",
        "laravel/framework": "^9.0|^10.0|^11.0",
        "spatie/geocoder": "^3.1",
        "ext-openssl": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Vemcogroup\\Weather\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Vemcogroup\\Weather\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vemcogroup\\Weather\\WeatherServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
